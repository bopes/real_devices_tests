  "scripts": {
    "build": "babel src --out-dir lib --source-maps && cp -r injections lib/",
    "build:clean": "rm -rf temp/* && rm -rf reports/* && rm -rf lib/* && npm run build",
    "build:test:clean": "rm -rf tests/lib/* && ./node_modules/.bin/babel tests/src --out-dir tests/lib --source-maps",
    "coverage": "npm run build:clean && npm run build:test:clean && ./node_modules/.bin/istanbul cover ./node_modules/mocha/bin/_mocha -- --recursive tests/lib",
    "lint": "eslint src/**/*.js",
    "postinstall": "[ -d src ] && npm run build:clean || exit 0",
    "prepublish": "npm run build:clean",
    "test": "npm run build:clean && npm run build:test:clean && ./node_modules/.bin/mocha --recursive tests/lib",
    "test:sauce": "magellan --external_build_id=$TRAVIS_BUILD_ID --sauce_create_tunnels --bail_time=${MAGELLAN_INDIVIDUAL_TEST_BAIL_TIME} --max_workers=${MAGELLAN_MAX_WORKERS} --sauce_browsers=${TIER_1_BROWSERS},${TIER_2_BROWSERS} --nightwatch_config tests/conf/nightwatch.json --sync_browsers safari:10,iphone,ipad --config tests/magellan.json",
    "upload-coverage": "codecov --root=lib"
  },
